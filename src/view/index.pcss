.pkg[data-pkg="agency-pkg-gyro/view"] {
  position: relative;
  height: 100%;
  float: left;
  box-sizing: border-box;
  border: solid #cccccc;
  border-width: 1px 1px 1px 0px;
  background-color: rgba(0, 0, 0, 0.05);


font-size: 11px;
.info {
  
}

  .bar {
    position: absolute;
    top: 0;
    height: 100%;
    background: rgba(255, 0, 0, 0.4);
    width: 40%;
    &.left {
      left: 0;
    }
    &.right {
      right: 0;
    }
  }
}
