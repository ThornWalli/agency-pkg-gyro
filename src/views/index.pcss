.pkg[data-pkg="agency-pkg-gyro/views"] {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border: solid #cccccc;
  border-width: 0px 1px 0px 1px;
  >.list {
    height: 100%;
  }
  * {
    user-select: none;
  }
  /* docs */
  .preview> & {
    position: relative;
    width: auto;
    margin: 0rem calc(30 / 16)rem;
    height: 30rem;
  }
}
