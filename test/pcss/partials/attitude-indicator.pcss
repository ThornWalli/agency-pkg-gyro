.partial[data-partial="attitude-indicator"] {
  position: fixed;
  top: 50%;
  left: 50%;
  width: 50%;
  transform: translate(-50%, -50%);
  background: #5b93c5;
&:before {
  content: "";
  display: block;
  padding-top: 100%;
}
border-radius: 50%;
overflow: hidden;
>div {
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}

  &, * {
    cursor: pointer;
    user-select: none;
  }
}
