.partial[data-partial="reset"] {
  color: #000000;
  [data-partial="test-cases"] & {
    color: #00ff00;
  }
  text-decoration: none;
  appearance: none;
  border: 0;
  background: transparent;
  font-size: 1rem;
  font-family: sans-serif;
  letter-spacing: 1px;
  text-transform: uppercase;
  position: absolute;
  bottom: 4%;
  right: 4%;

  outline: none;
}
